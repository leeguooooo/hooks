{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"jsx": "react",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"outDir": "./dist"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}
